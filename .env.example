# ============================================================
# WSL Xray Client - Environment Configuration Template
# ============================================================
# Copy this file to .env and fill in your values
# IMPORTANT: NEVER commit .env file to version control!
# Add .env to .gitignore before committing
# ============================================================

# INBOUND CONFIGURATION (Connection to 3X-UI Server)
XRAY_PROTOCOL=vless
XRAY_SERVER_ADDRESS=vpn.example.com
XRAY_SERVER_PORT=443
XRAY_SERVER_UUID=a1b2c3d4-e5f6-7890-abcd-ef1234567890
XRAY_SECURITY=tls
XRAY_NETWORK=tcp

# OUTBOUND CONFIGURATION (Local Proxy Output)
XRAY_OUTBOUND_PROTOCOL=socks5
XRAY_LISTEN_IP=127.0.0.1
XRAY_OUTBOUND_PORT=1080
XRAY_HTTP_PORT=8080

# SERVICE CONFIGURATION
XRAY_USER=xray
XRAY_SERVICE_RESTART_ON_FAILURE=true
XRAY_AUTO_START=true

# LOGGING CONFIGURATION
XRAY_LOG_LEVEL=warning
XRAY_LOG_ACCESS=none
XRAY_LOG_ERROR=/var/log/xray/error.log
XRAY_DNS_LOG=false

# ADVANCED CONFIGURATION (Optional)
XRAY_TIMEOUT=300
XRAY_STATS_ENABLED=false
XRAY_DNS_SERVER=8.8.8.8
XRAY_ROUTING_ENABLED=true
XRAY_MUX_ENABLED=false
XRAY_TCP_FAST_OPEN=true

# INSTALLATION OPTIONS
XRAY_BINARY_PATH=/usr/local/bin/xray
XRAY_CONFIG_DIR=/etc/xray
XRAY_DATA_DIR=/var/lib/xray
XRAY_LOG_DIR=/var/log/xray